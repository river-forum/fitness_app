{
  "name": "front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint --fix **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "test:jest-update": "jest --updateSnapshot",
    "storybook:dev": "yarn nuxt storybook",
    "storybook:build": "yarn nuxt storybook build"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "2.0.1",
    "@nuxtjs/axios": "5.13.1",
    "@nuxtjs/composition-api": "0.19.1",
    "@nuxtjs/pwa": "3.3.5",
    "core-js": "3.8.3",
    "lodash": "4.17.20",
    "moment": "2.29.1",
    "node-sass": "5.0.0",
    "nuxt": "2.14.12",
    "sass-loader": "10.1.1",
    "vuejs-datepicker": "1.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@nuxt/types": "2.14.12",
    "@nuxt/typescript-build": "2.0.4",
    "@nuxtjs/eslint-config": "5.0.0",
    "@nuxtjs/eslint-config-typescript": "5.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/storybook": "3.3.1",
    "@nuxtjs/style-resources": "1.0.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/tailwindcss": "3.4.2",
    "@storybook/addon-controls": "6.1.17",
    "@storybook/addon-docs": "6.1.17",
    "@types/moment": "2.13.0",
    "@vue/test-utils": "1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.3.1",
    "fibers": "5.0.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "sass": "1.32.7",
    "stylelint": "13.10.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "vue-jest": "3.0.7"
  }
}
