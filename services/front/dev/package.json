{
  "name": "front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint --fix **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "test:jest-update": "jest --updateSnapshot",
    "storybook:dev": "yarn nuxt storybook",
    "storybook:build": "yarn nuxt storybook build"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "2.1.0",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/composition-api": "0.24.4",
    "@nuxtjs/pwa": "3.3.5",
    "core-js": "3.14.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "node-sass": "6.0.0",
    "nuxt": "2.15.7",
    "sass-loader": "12.1.0",
    "vuejs-datepicker": "1.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@nuxt/types": "2.15.7",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config": "6.0.1",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/storybook": "4.0.4",
    "@nuxtjs/style-resources": "1.1.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/tailwindcss": "4.1.3",
    "@storybook/addon-controls": "6.2.9",
    "@storybook/addon-docs": "6.2.9",
    "@types/moment": "2.13.0",
    "@vue/test-utils": "1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.2",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "fibers": "5.0.0",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "sass": "1.34.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "vue-jest": "3.0.7"
  }
}
